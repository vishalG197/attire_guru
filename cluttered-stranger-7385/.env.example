# Copy this file to .env and update the values

# API Configuration
# The base URL for the JSON Server or backend API
REACT_APP_API_URL=http://localhost:8080

# API request timeout in milliseconds
REACT_APP_API_TIMEOUT=10000

# App Configuration
# Application name
REACT_APP_NAME=Attire Guru

# Application version
REACT_APP_VERSION=0.1.0

# Feature Flags
# Enable/disable debug mode and console logs
REACT_APP_DEBUG_MODE=false
